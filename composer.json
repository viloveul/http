{
    "name": "viloveul/http",
    "description": "Http Component of Viloveul, Implementation PSR HTTP Message with Zend Diactoros",
    "type": "library",
    "license": "MIT",
    "keywords": ["zafex", "viloveul", "fajrulaz", "http", "component", "library"],
    "homepage": "http://www.viloveul.com",
    "readme": "README.MD",
    "authors": [{
        "name": "Fajrul Akbar Zuhdi",
        "email": "fajrulaz@gmail.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Viloveul\\Http\\": "src/"
        }
    },
    "require": {
        "php": "~7.0",
        "psr/http-message": "^1.0",
        "zendframework/zend-diactoros": "2.1.*"
    },
    "require-dev": {
        "symfony/var-dumper": "~3.4|~4.0"
    }
}
